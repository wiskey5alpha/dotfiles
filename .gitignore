# Lots of junk goes in here.
/caches/
/backups/

# Top secret stuff!
/link/.ssh/id_*
/link/.ssh/*.pem
/link/.ssh/known_hosts*

# Ignore autogenerated emacs files
/link/.spacemacs.d/emacs-customs.el
/link/.spacemacs.d/config/*.el

# Don't commit rbenv temp files.
/link/.rbenv/shims/
/link/.rbenv/versions/
/link/.rbenv/version

# Don't commit tmux plugins, tpm will install them.
/link/.tmux/plugins

# the onedrive client has a config file, but also some state databases
# we can ignore anything besides the config
/config/onedrive/*sqlite3*
/config/onedrive/refresh_token
# Ignore any file with LOCAL in the name (good for per-machine vars/functions)
*LOCAL*
/link/.fonts/.uuid
